      {
        "id": "minecraft",
        "versions": ">=1.18.2"
      },
      {
        "id": "cloth-config2",
        "versions": ">=6.2.57"
      },
      {
        "id": "cloth-feature_nbt_deadlock_be_gone",
        "versions": "*"
      }
    ]
